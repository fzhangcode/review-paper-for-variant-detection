issn = {15537390},
journal = {PLoS Genetics},
number = {11},
pmid = {24278038},
title = {{Whole Genome, Whole Population Sequencing Reveals That Loss of Signaling Networks Is the Major Adaptive Strategy in a Constant Environment}},
volume = {9},
year = {2013}
}
