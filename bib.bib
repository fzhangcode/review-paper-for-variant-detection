file = {:C$\backslash$:/Users/fan zhang/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Liu, Leal - 2012 - Seqchip A powerful method to integrate sequence and genotype data for the detection of rare variant associations.pdf:pdf},
