file = {:Z$\backslash$:/fzhang/Research/papers{\_}books/Variants{\_}detection/Rare Variants Detection Methods/feature based classifiers for somatic mutation detection in tumour-normal paired sequencing data .full.pdf:pdf},
isbn = {1367-4811 (Electronic)$\backslash$r1367-4803 (Linking)},
issn = {13674803},
journal = {Bioinformatics},
number = {2},
pages = {167--175},
pmid = {22084253},
title = {{Feature-based classifiers for somatic mutation detection in tumour-normal paired sequencing data}},
volume = {28},
year = {2012}
}
